{
  "name": "oak-web-application",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "NODE_ENV=production node scripts/build/write_env_file",
    "build": "next build",
    "build:analyse-bundle": "ANALYSE_BUNDLE=on next build",
    "postbuild": "next-sitemap",
    "cc": "commit",
    "cca": "git add . && commit",
    "check-types": "tsc --noEmit",
    "config:pull": "node ./scripts/build/pull_config",
    "config:push:dev": "node ./scripts/build/push_config --environment development",
    "config:push:prod": "node ./scripts/build/push_config --environment production",
    "config:push:test": "node ./scripts/build/push_config --environment test",
    "predev": "node scripts/build/write_env_file",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier '**/*.{j,t}{s,sx}' --write",
    "format:check": "prettier '**/*.{j,t}{s,sx}' --check",
    "gql-codegen": "npm run gql-codegen:curriculum",
    "gql-codegen:curriculum": "graphql-codegen --config src/node-lib/curriculum-api/codegen.yml -r dotenv/config",
    "gql-codegen:sanity": "graphql-codegen --config src/node-lib/sanity-graphql/codegen.yml -r dotenv/config",
    "lint": "NODE_ENV=test next lint",
    "lint:styles": "stylelint 'src/**/*.{j,t}{s,sx}'",
    "pa11y": "pa11y-ci -c pa11yci.config.js",
    "playwright:install": "npx playwright install #install Playwright binaries on system",
    "playwright:test": "LOCAL_E2E=on npx playwright test",
    "playwright:test:ci": "npx playwright test --config=playwright.browserstack.config.ts",
    "playwright:test:bs_local": "LOCAL_E2E=on npx playwright test --config=playwright.browserstack.config.ts",
    "playwright:show": "npx playwright show-report",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "serve:test-config": "http-server -p 4444 oak-config",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:static-serve": "build-storybook --quiet && http-server ./storybook-static --port=6006",
    "test": "jest --watch --collectCoverage=false",
    "test:ci": "jest",
    "test:storybook": "jest --config=jest.storybook.config.js",
    "test:storybook:ci": "start-server-and-test storybook:static-serve http://localhost:6006 test:storybook"
  },
  "overrides": {
    "puppeteer": "$puppeteer",
    "react": "$react",
    "react-dom": "$react-dom",
    "storybook-addon-next-router": {
      "next": "$next"
    }
  },
  "dependencies": {
    "@bugsnag/js": "^7.18.0",
    "@bugsnag/plugin-react": "^7.18.0",
    "@hookform/resolvers": "^2.9.10",
    "@hubspot/api-client": "^7.1.2",
    "@mux/mux-player-react": "^1.3.0",
    "@portabletext/react": "^1.0.6",
    "@react-aria/aria-modal-polyfill": "^3.6.0",
    "@sanity/image-url": "^1.0.1",
    "@types/uuid": "^8.3.4",
    "change-case": "^4.1.2",
    "date-fns": "^2.29.3",
    "gleap": "^9.0.2",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^13.1.6",
    "next-sanity-image": "^5.0.0",
    "next-seo": "^5.15.0",
    "node-fetch": "^2.6.7",
    "posthog-js": "^1.34.0",
    "react": "^18.2.0",
    "react-aria": "^3.20.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-focus-on": "^3.6.0",
    "react-hook-form": "^7.39.1",
    "react-stately": "^3.18.0",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.6",
    "swr": "^2.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@axe-core/react": "^4.5.1",
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/prompt-cli": "^17.2.0",
    "@google-cloud/secret-manager": "^4.1.3",
    "@graphql-codegen/cli": "^2.13.11",
