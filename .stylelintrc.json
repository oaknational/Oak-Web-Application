{
  "extends": ["stylelint-config-standard-scss"],
  "customSyntax": "postcss-styled-syntax",
  "rules": {
    "selector-type-no-unknown": [
      true,
      { "ignoreTypes": ["/-styled-mixin/", "/^\\$\\w+/"] }
    ],
    "value-keyword-case": ["lower", { "ignoreKeywords": ["dummyValue"] }],
    "keyframes-name-pattern": null
  }
}
